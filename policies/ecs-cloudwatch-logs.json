{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
         "logs:CreateLogGroup",
         "logs:CreateLogStream",
         "logs:PutLogEvents",
         "logs:DescribeLogStreams",
         "logs:PutRetentionPolicy"
      ],
      "Resource": [
         "arn:aws:logs:*:*:*"
      ]
    }
  ]
}
